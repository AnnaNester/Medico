<Window x:Class="MedicoView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedicoView"
        mc:Ignorable="d"
        Title="Inicio" Height="400" Width="600">
    <Grid>
        <Button x:Name="btnCadastro" Content="Cadastro" HorizontalAlignment="Left" Margin="247,98,0,0" VerticalAlignment="Top" Width="96" Height="34" Click="btnCadastro_Click"/>
        <Button x:Name="btnAlterar" Content="Alterar" HorizontalAlignment="Left" Margin="247,180,0,0" VerticalAlignment="Top"  Width="96" Height="34"/>
        <Button x:Name="btnPesquisar" Content="Pesquisar" HorizontalAlignment="Left" Margin="247,264,0,0" VerticalAlignment="Top"  Width="96" Height="34" Click="btnPesquisar_Click"/>
        <Label x:Name="label" Content="Médicos" HorizontalAlignment="Left" Margin="247,26,0,0" VerticalAlignment="Top" Width="96" FontSize="20" FontFamily="Segoe Script"/>

    </Grid>
